{
  "name": "epic-ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "sharp"
    ]
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @epic-ai/database generate",
    "db:push": "pnpm --filter @epic-ai/database push",
    "db:migrate": "pnpm --filter @epic-ai/database migrate",
    "db:studio": "pnpm --filter @epic-ai/database studio"
  },
  "devDependencies": {
    "next": "15.1.9",
    "turbo": "^2.3.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@vercel/blob": "^2.0.0",
    "react-dropzone": "^14.3.8"
  }
}
